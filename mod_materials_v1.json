{
  "swagger": "2.0",
  "info": {
    "description": "<a href=\"https://wiki.edu.mos.ru/pages/viewpage.action?pageId=56957683\">211 - MaterialProviderRegistry. Описание API</a>",
    "version": "1.0",
    "title": "API mod_materials"
  },
  "tags": [
    {
      "name": "filter-controller",
      "description": "Запросы и фильтрация списка материалов с указанием идентификатора партнёра"
    },
    {
      "name": "material-uuid-controller",
      "description": "Запросы информации о составе набранных материалов по UUID"
    },
    {
      "name": "partner-controller",
      "description": "Запросы информации о партнерах провайдерах материалов"
    }
  ],
  "paths": {
    "/material/{uuid}": {
      "get": {
        "tags": [
          "material-uuid-controller"
        ],
        "summary": "findMaterialByUuid",
        "operationId": "findMaterialByUuidUsingGET",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "uuid",
            "in": "path",
            "description": "UUID-идентификатор набора материалов",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/Material"
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          }
        },
        "deprecated": false
      }
    },
    "/materials/partner": {
      "get": {
        "tags": [
          "partner-controller"
        ],
        "summary": "getPartners",
        "operationId": "getPartnersUsingGET",
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Partner"
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          }
        },
        "deprecated": false
      }
    },
    "/materials/{partner_id}": {
      "post": {
        "tags": [
          "filter-controller"
        ],
        "summary": "filter",
        "operationId": "filterUsingPOST",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "partner_id",
            "in": "path",
            "description": "Идентификатор провайдера материалов",
            "required": true,
            "type": "integer",
            "format": "int64"
          },
          {
            "in": "body",
            "name": "filterParams",
            "description": "Объект с параметрами фильтрации материалов",
            "required": true,
            "schema": {
              "$ref": "#/definitions/FilterParams"
            }
          },
          {
            "name": "page",
            "description": "Номер страницы",
            "default": 0,
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "size",
            "description": "Количество материалов на странице",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32",
            "default": 10
          },
          {
            "name": "sort",
            "description": "Поля/направление сортировки (title,rating(,asc|desc))",
            "in": "query",
            "required": false,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/Page%C2%ABMaterial%C2%BB"
            }
          },
          "201": {
            "description": "Created"
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          }
        },
        "deprecated": false
      }
    }
  },
  "definitions": {
    "FilterParams": {
      "type": "object",
      "description": "Объект с параметрами запроса и фильтрации по материалам",
      "properties": {
        "parallell_ids": {
          "description": "Массив параллелей",
          "type": "array",
          "items": {
            "type": "integer",
            "format": "int64"
          }
        },
        "subject_ids": {
          "description": "Массив предметов",
          "type": "array",
          "items": {
            "type": "integer",
            "format": "int64"
          }
        },
        "didactic_unit_ids": {
          "description": "Массив дидактических единиц",
          "type": "array",
          "items": {
            "type": "integer",
            "format": "int64"
          }
        },
        "material_types": {
          "description": "Идентификаторы типов материала",
          "type": "array",
          "items": {
            "type": "string",
            "enum": [
              "lesson_templates",
              "game_apps",
              "test_specifications",
              "composed_documents",
              "atomic_objects",
              "literature"
            ]
          }
        }
      },
      "title": "FilterParams"
    },
    "Material": {
      "type": "object",
      "description": "Данные о модели материала на витрине, содержит uuid/source_url материала",
      "properties": {
        "author": {
          "type": "string",
          "description": "автор"
        },
        "description": {
          "type": "string",
          "description": "описание"
        },
        "icon_url": {
          "type": "string",
          "description": "URL изображения"
        },
        "uuid": {
          "type": "string",
          "description": "uuid-идентификатор"
        },
        "name": {
          "type": "string",
          "description": "название"
        },
        "rating": {
          "type": "integer",
          "format": "int32",
          "description": "рейтинг"
        },
        "source_url": {
          "type": "string",
          "description": "URL ссылки"
        },
        "subject_ids": {
          "description": "Идентификаторы предметов",
          "type": "array",
          "items": {
            "type": "integer",
            "format": "int64"
          }
        },
        "type": {
          "type": "string"
        }
      },
      "title": "Material"
    },
    "Pageable": {
      "type": "object",
      "description": "Параметры постраничного вывода данных",
      "properties": {
        "pageNumber": {
          "type": "integer",
          "format": "int32",
          "description": "номер страницы"
        },
        "pageSize": {
          "type": "integer",
          "format": "int32",
          "description": "размер страницы"
        }
      },
      "title": "Pageable"
    },
    "Page«Material»": {
      "type": "object",
      "description": "Материалы в постраничном представлении",
      "properties": {
        "content": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Material"
          },
          "description": "Материалы страницы"
        },
        "pageable": {
          "$ref": "#/definitions/Pageable"
        },
        "totalElements": {
          "type": "integer",
          "format": "int64",
          "description": "общее число материалов"
        },
        "totalPages": {
          "type": "integer",
          "format": "int32",
          "description": "общее число страниц"
        }
      },
      "title": "Page«Material»"
    },
    "Partner": {
      "type": "object",
      "description": "Информация о партнере провайдере материалов",
      "properties": {
        "description": {
          "type": "string",
          "description": "текст описания"
        },
        "partner_icon_url": {
          "type": "string",
          "description": "URL изображения"
        },
        "partner_id": {
          "type": "integer",
          "format": "int64",
          "description": "идентификатор"
        },
        "partner_name": {
          "type": "string",
          "description": "название"
        },
        "redirect_url": {
          "type": "string",
          "description": "URL страницы партнера"
        }
      },
      "title": "Partner"
    }
  }
}